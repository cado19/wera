<section class="content">
	<div class="box box-primary">
		<div class="box box-body">
			<p>Account Name: <%= current_account.name %></p>
			<p>Subdomain: <%= current_account.subdomain%></p>
			<p>Users: <%= "#{number_of_users}" %></p>
			<p>Products: <%= "#{number_of_products}" %></p>
			<p>Sales: <%= "#{number_of_sales}" %></p>
			<p>Created at: <%= current_account.created_at.strftime("%B %d %Y") %>			
		</div>
	</div>
</section>