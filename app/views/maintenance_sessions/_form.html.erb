<%= form_tag maintenance_login_url do %>
	<%= label_tag :email %>
	<%= text_field_tag :email, params[:password], class: 'form-control' %><br>

	<%= label_tag :password %>
	<%= password_field_tag :password, params[:password], class: 'form-control'  %><br>

	<%= button_tag 'Login', class: 'btn btn-info', data: {disable_with: "<i class='fa fa-spinner fa-spin'></i> Loggin In..."} %> 
<% end %>
