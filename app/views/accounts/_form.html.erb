<%= form_for @account  do |f|%>
	<div class="">
		<%= f.label :name %>
		<%= f.text_field :name, class: 'form-control'%><br>

		<%= f.label :subdomain %>
		<%= f.text_field :subdomain, class: 'form-control'%><br>

		<%= f.label :country %>
		<%= f.select :country, ['Kenya', 'Uganda', 'Tanzania'], class: 'form-control' %><br>

		<%= f.submit "Create Account", class: 'btn btn-primary'%>
		<%= link_to 'Cancel', welcome_home_url, class: 'btn btn-danger' %>
	</div>
<% end %>
