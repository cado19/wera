<%= form_tag login_url do %>
	<%= label_tag :email %>
	<%= text_field_tag :email, params[:password], class: 'form-control' %><br>

	<%= label_tag :password %>
	<%= password_field_tag :password, params[:password], class: 'form-control'  %><br>

	<%= submit_tag 'Login', class: 'btn btn-info form-control' %> 
<% end %>
