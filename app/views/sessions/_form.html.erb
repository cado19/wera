<%= bootstrap_form_tag url: '/login' do %>
	<%= text_field_tag :email, params[:email], class: 'form-control', placeholder: "Email Address" %><br>

	<%= password_field_tag :password, params[:password], class: 'form-control', placeholder: "Password"  %><br>

	<%= submit_tag 'Login', class: 'btn btn-info' %> 
<% end %>
